
public class CubeSumAssignment {
	int n,r,sum;
	public void findSum() {
		while(n!=0) {
			r=n%10;
			sum=sum+(r*r*r);
			n=n/10;
		}
		System.out.println("The sum is "+ sum);
	}
	public static void main(String args[]) {
        CubeSumAssignment obj=new CubeSumAssignment();
		obj.findSum();
	}

}
